{
  "name": "ibr-sdk",
  "version": "0.1.0",
  "description": "Internal Building Representation SDK",
  "main": "src/index.js",
  "author": "Shuang Li",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run lint && npm install && npm run build-proto && rollup -c rollup.config.js",
    "build-proto": " mkdir -p temp/ && node_modules/.bin/pbf proto/ibr.proto --browser > temp/ibr_pb_browser.js",
    "cleanup": "rm -rf dist && rm -rf node_modules && rm -rf temp && rm -rf package-lock.json",
    "lint": "npx eslint src/** --fix",
    "test": "ava"
  },
  "dependencies": {
    "three": "^0.117.1",
    "three-buffer-geometry-utils": "^1.0.0",
    "pbf": "^3.2.1"
  },
  "devDependencies": {
    "@webpack-cli/init": "^0.3.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "html-webpack-plugin": "^4.3.0",
    "terser-webpack-plugin": "^3.0.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "files": [
      "test/*.js"
    ]
  }
}
